{
  "name": "@validitor/main",
  "version": "0.0.9",
  "description": "typing and validation for javascript",
  "main": "./public/cjs/main.js",
  "module": "./public/esm/main.js",
  "exports": {
    ".": {
      "require": "./public/cjs/main.js",
      "import": "./public/esm/main.js"
    },
    "./validate": {
      "require": "./public/cjs/Validate.js",
      "import": "./public/esm/Validate.js"
    },
    "./verify": {
      "require": "./public/cjs/Verify.js",
      "import": "./public/esm/Verify.js"
    },
    "./type": {
      "require": "./public/cjs/Type.js",
      "import": "./public/esm/Type.js"
    },
    "./types": {
      "require": "./public/cjs/Types.js",
      "import": "./public/esm/Types.js"
    },
    "./interface": {
      "require": "./public/cjs/Interface.js",
      "import": "./public/esm/Interface.js"
    },
    "./onlyif": {
      "require": "./public/cjs/onlyIf.js",
      "import": "./public/esm/onlyIf.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FreenetProject/must.git"
  },
  "keywords": [
    "require",
    "validation",
    "validator",
    "interface",
    "typing"
  ],
  "homepage": "https://github.com/FreenetProject/must",
  "author": "Lluie Cain Andaya",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true
}
